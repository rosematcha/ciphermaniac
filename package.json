{
  "name": "ciphermaniac",
  "version": "0.1.0",
  "private": true,
  "description": "Pokemon TCG tournament data visualization tool",
  "main": "assets/js/main.js",
  "scripts": {
    "lint": "eslint assets/js --ext .js",
    "lint:fix": "eslint assets/js --ext .js --fix",
    "format": "prettier --write \"assets/js/**/*.js\"",
    "format:check": "prettier --check \"assets/js/**/*.js\"",
    "typecheck": "npx tsc --noEmit --project jsconfig.json",
    "jsdoc": "jsdoc -c jsdoc.config.js",
    "jsdoc:validate": "eslint assets/js --ext .js --rule 'valid-jsdoc: error'",
    "complexity": "plato -r -d complexity-report assets/js",
    "dev": "node test-server.js",
    "test": "npm run test:routing",
    "test:routing": "node --test --loader ./tests/esm-test-loader.mjs tests/card-routing.test.mjs",
    "validate": "npm run lint && npm run typecheck && npm run format:check",
    "validate:fix": "npm run lint:fix && npm run format && npm run typecheck",
    "clean": "echo 'No build artifacts to clean'",
    "pre-commit": "npm run validate:fix"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^48.0.0",
    "jsdoc": "^4.0.0",
    "plato": "^1.7.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "pokemon",
    "tcg",
    "tournament",
    "data-visualization",
    "frontend"
  ],
  "author": "Ciphermaniac",
  "license": "MIT"
}
