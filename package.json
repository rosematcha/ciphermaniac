{
  "name": "ciphermaniac",
  "version": "0.1.0",
  "private": true,
  "description": "Pokemon TCG tournament data visualization tool",
  "main": "public/assets/js/main.js",
  "scripts": {
    "lint": "eslint public/assets/js --ext .js",
    "lint:fix": "eslint public/assets/js --ext .js --fix",
    "typecheck": "npx tsc --noEmit --project config/jsconfig.json",
    "jsdoc": "jsdoc -c config/jsdoc.config.js",
    "jsdoc:validate": "eslint public/assets/js --ext .js --rule 'valid-jsdoc: error'",
    "complexity": "plato -r -d complexity-report public/assets/js",
    "dev": "node dev/test-server.js",
    "test": "npm run test:routing",
    "test:routing": "node --test --loader ./dev/tests/esm-test-loader.mjs dev/tests/card-routing.test.mjs",
    "validate": "npm run lint && npm run typecheck",
    "validate:fix": "npm run lint:fix && npm run typecheck",
    "clean": "echo 'No build artifacts to clean'",
    "pre-commit": "npm run validate:fix"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^48.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdoc": "^4.0.0",
    "plato": "^1.7.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "pokemon",
    "tcg",
    "tournament",
    "data-visualization",
    "frontend"
  ],
  "author": "Ciphermaniac",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.928.0"
  }
}
